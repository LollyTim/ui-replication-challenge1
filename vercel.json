{
  "version": 2,
  "routes": [
    {
      "src": "^/$",
      "dest": "/"
    },
    {
      "src": "^/grayV$",
      "dest": "/"
    },
    {
      "src": "^/blog$",
      "dest": "/blog/index"
    },
    {
      "src": "^/blog/(?<slug>[^/]+)$",
      "dest": "/blog/post?slug=$slug"
    },
    {
      "src": "^/products/(?<id>\\d+)$",
      "dest": "/products?id=$id"
    },
    {
      "src": "^/old-route$",
      "status": 301,
      "headers": {
        "Location": "/new-route"
      }
    },
    {
      "src": "^/(.*)",
      "dest": "/$1",
      "has": [
        {
          "type": "cookie",
          "name": "session_id"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/old-blog",
      "destination": "/blog",
      "permanent": true
    },
    {
      "source": "/deprecated",
      "destination": "/new-feature",
      "statusCode": 302
    }
  ],
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/app/dashboard"
    },
    {
      "source": "/products/(?<category>[^/]+)",
      "destination": "/products?category=$category"
    }
  ],
  "headers": [
    {
      "source": "/app/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
