{
  "version": 2,
  "routes": [
    {
      "src": "^/$",
      "dest": "/"
    },
    {
      "src": "^/grayV$",
      "dest": "/"
    },
    {
      "src": "^/blog$",
      "dest": "/blog/index"
    },
    {
      "src": "^/blog/(?<slug>[^/]+)$",
      "dest": "/blog/post?slug=$slug"
    },
    {
      "src": "^/products/(?<id>\\d+)$",
      "dest": "/products?id=$id"
    },
    {
      "src": "^/(.*)",
      "dest": "/$1",
      "has": [
        {
          "type": "cookie",
          "name": "session_id"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
